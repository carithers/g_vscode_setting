  "doxdocgen.generic.order": [
    "brief",
    "tparam",
    "param",
    "return"
  ],
  "doxdocgen.generic.paramTemplate": "@param{indent:8}{param}{indent:25}My Param doc",
  "doxdocgen.generic.returnTemplate": "@return {type} ",
