{
  "C_Cpp.clang_format_fallbackStyle": "Microsoft",
  "doxdocgen.file.copyrightTag": [
    "@copyright Copyright (c) {year}  c"
  ],
  "doxdocgen.file.customTag": [
    "@par Changelogs:",
    "<table>",
    "<tr><th>Date       <th>Version <th>Author  <th>Description",
    "<tr><td>{date} <td>1.0     <td>{author}     <td>log",
    "</table>"
  ],
  // 文件注释的组成及其排序
  "doxdocgen.file.fileOrder": [
      "file",		// @file
      "brief",	// @brief 简介
      "author",	// 作者
      "version",	// 版本
      "date",		// 日期
      "empty",	// 空行
      "copyright",// 版权
      "empty",
      "custom"	// 自定义
  ],
  "doxdocgen.file.fileTemplate": "@file {name}",
  "doxdocgen.file.versionTag": "@version 1.0",
  "doxdocgen.generic.authorEmail": "carithers@163.com",
  "doxdocgen.generic.authorName": "c",
  "doxdocgen.generic.authorTag": "@author {author} ({email})",
  // 根据自动生成的注释模板（目前主要体现在函数注释上）
  "doxdocgen.generic.order": [
    "brief",
    "tparam",
    "param",
    "return"
  ],
  "doxdocgen.generic.paramTemplate": "@param{indent:8}{param}{indent:25}My Param doc",
  "doxdocgen.generic.returnTemplate": "@return {type} ",
  "doxdocgen.generic.splitCasingSmartText": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vscode-neovim.neovimInitVimPaths.win32": "~\\AppData\\Local\\nvim\\init.lua",
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape"
    },
    ";;": {
      "command": "vscode-neovim.send",
      "args": ["<esc>A;"]
    },
  },
  "vscode-neovim.ctrlKeysForNormalMode": [
    "a",
    "b",
    "c",
    "d",
    "e",
    "f",
    "h",
    "i",
    "k",
    "l",
    "m",
    "o",
    "r",
    "t",
    "u",
    "v",
    "w",
    "x",
    "y",
    "/",
    "]",
    "right",
    "left",
    "up",
    "down",
    "backspace",
    "delete"
  ],
  "workbench.colorTheme": "Atom One Dark",
  "EIDE.ARM.GCC.InstallDirectory": "~/.eide/tools/gcc_arm",
  "EIDE.JLink.InstallDirectory": "${userRoot}/.eide/tools/jlink",
  "EIDE.OpenOCD.ExePath": "~/.eide/tools/openocd_7a1adfbec_mingw32/bin/openocd.exe",
  "cortex-debug.liveWatchRefreshRate": 500,
  "cortex-debug.armToolchainPath": "${userHome}/.eide/tools/gcc_arm/bin",
  "cortex-debug.armToolchainPath.windows": "${userHome}/.eide/tools/gcc_arm/bin",
  "cortex-debug.armToolchainPrefix": "arm-none-eabi",
  "cortex-debug.JLinkGDBServerPath": "${userHome}/.eide/tools/jlink/JLinkGDBServerCL.exe",
  "cortex-debug.gdbPath": "${userHome}/.eide/tools/gcc_arm/bin/arm-none-eabi-gdb.exe",
  "cortex-debug.openocdPath": "${userHome}/.eide/tools/openocd_7a1adfbec_mingw32/bin/openocd.exe",
}
